<div class = "evs show">
	<h2>Show Ev</h2>
	<table summary="user detail view" >
        <tr>
			<th>Admin</th>
			<td><%= @ev.username %></td>
		</tr>
        <tr>
			<th>Name</th>
			<td><%= @ev.name %></td>
		</tr>
		<tr>
			<th>Date</th>
			<td><%= @ev.date %></td>
		</tr>
		<tr>
			<th>Description</th>
			<td><%= @ev.description %></td>
		</tr>
		<tr>
			<th>Location</th>
			<td><%= @ev.location %></td>
		</tr>
		<tr>
			<th>Users</th>
			<td><%= @ev.users.size %></td>
		</tr>
		<tr>
			<th>Created</th>
			<td><%= @ev.created_at %></td>
		</tr>
		<tr>
			<th>Updated</th>
			<td><%= @ev.updated_at %></td>
		</tr>
		    
	</table>
</div>